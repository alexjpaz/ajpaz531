<?xml version="1.0" encoding="UTF-8"?>
<groups xmlns="http://www.isdc.ro/wro" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.isdc.ro/wro wro.xsd">
    <!--http://code.google.com/p/wro4j/wiki/WroFileFormat-->
	<group name="app">
		<js>/assets/config/app/**.js</js>

		<!-- Providers: Order matters -->
		<js>/assets/providers/StringHelper.js</js>
		<js>/assets/providers/TemplateResolver.js</js>
		<js>/assets/providers/ComponentTemplateResolver.js</js>
		<js>/assets/providers/Directive.js</js>
		<js>/assets/providers/Screen.js</js>
		<js>/assets/providers/Resource.js</js>
		<js>/assets/providers/Component.js</js>
		<js>/assets/providers/RouteScreen.js</js>
		<!-- /Providers -->

		<js>/assets/providers/**.js</js>

		<js>/assets/config/routes/**.js</js>
		<js>/assets/config/links/**.js</js>
		<js>/assets/config/api/**.js</js>

		<js>/assets/helpers/**.js</js>
		<js>/assets/components/**.js</js>
		<js>/assets/directives/**.js</js>
		<js>/assets/filters/**.js</js>
		<js>/assets/screen/**.js</js>

		<css>/assets/css/common/**.css</css>
		<css>/assets/components/**.css</css>
		<css>/assets/components/**.less</css>
		<css>/assets/screen/**.css</css>
		<css>/assets/screen/**.less</css>
	</group>
</groups>
